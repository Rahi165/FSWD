<body>
    <p id="var"></p>
    <p id="let"></p>
    <p id="const"></p>
    <script>
    // Using var
    var myVar = "Hello";
    document.getElementById("var").innerHTML=myVar;
   

    // Using let
    let myLet = 42;
    document.getElementById("let").innerHTML=myLet;

    // Using const
    const myConst = true;
    document.getElementById("const").innerHTML=myConst;

    </script>
</body>
</html>
